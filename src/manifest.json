{
  "manifest_version": 3,

  "name": "ChatGPT-Simple",
  "description": "Simply call ChatGPT",
  "version": "1.0",

  "permissions": [
    "storage"
  ],

  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },

  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
}
